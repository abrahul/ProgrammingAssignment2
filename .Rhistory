## This function finds the inverse of the special "matrix" made by makeCacheMatrix
##If inverse is already calculated and matrix in unchanged,thenit should returns the inverse from the cache
cacheSolve <- function(x, ...) {
## Return a matrix that is the inverse of 'x'
inv <- x$getInverse()
if (!is.null(inv)) {
message("getting cached data")
return(inv)
}
mat <- x$get()
inv <- solve(mat, ...)
x$setInverse(inv)
inv
}
